{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "js": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommend",
    "plugin:react/recommended"
  ],
  "rules": {
    "consistent-return": 2,
    "indent": [
      1,
      4
    ],
    "no-else-return": 1,
    "semi": [
      1,
      "always"
    ],
    "space-unary-ops": 2
  }
}